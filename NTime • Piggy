local AkaliNotif = loadstring(game:HttpGet("https://raw.githubusercontent.com/Kinlei/Dynissimo/main/Scripts/AkaliNotif.lua"))();
local Notify = AkaliNotif.Notify;
Notify({
Description = game.Players.LocalPlayer.DisplayName;
Title = "Welcome To The NTime!";
Duration = 10;
});
wait()

local UILib = loadstring(game:HttpGet('https://raw.githubusercontent.com/inceldom/kinx/main/ui'))()

local win = UILib:Window("NTime â€¢ Piggy",Color3.fromRGB(255, 255, 255), Enum.KeyCode.V)

A1 = win:Tab("Tools")
A2 = win:Tab("Map")
A3 = win:Tab("Toggles")

A1:Button("FireClickDetector Tool", function()
tool = Instance.new("Tool")
tool.RequiresHandle = false
tool.Name = "FireCd"
tool.Activated:connect(function()
if fireclickdetector then
		for i,v in pairs(workspace:GetDescendants()) do
			if v:IsA("ClickDetector") then
				fireclickdetector(v)
			end
		end
end
end)
tool.Parent = game.Players.LocalPlayer.Backpack
end)

A1:Button("Teleport Tool", function()
mouse = game.Players.LocalPlayer:GetMouse()
tool = Instance.new("Tool")
tool.RequiresHandle = false
tool.Name = "Tp Tool"
tool.Activated:connect(function()
local pos = mouse.Hit+Vector3.new(0,2.5,0)
pos = CFrame.new(pos.X,pos.Y,pos.Z)
game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = pos
end)
tool.Parent = game.Players.LocalPlayer.Backpack
end)

A2:Dropdown("Map Selector",{"House", "Station", "Gallery", "Forest", "School", "Hospital", "Underground", "Carnival", "City", "Mall", "Outpost", "plant"}, function(t)
-- Script generated by SimpleSpy - credits to exx#9394

local args = {
    [1] = "Map",
    [2] = t
}

game:GetService("ReplicatedStorage").Remotes.NewVote:FireServer(unpack(args))
end)

A3:Toggle("FireClickDetector",false, function(t)
Firec = t
while Firec do
wait(0.001)
if fireclickdetector then
		for i,v in pairs(workspace:GetDescendants()) do
			if v:IsA("ClickDetector") then
				fireclickdetector(v)
			end
		end
end
end
end)
